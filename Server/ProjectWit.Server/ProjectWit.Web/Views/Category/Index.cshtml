@model IEnumerable<ProjectWit.Model.Wit_Category>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_MyAccountLayout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CategoryName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Wit_Company.CompanyName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CategoryName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Wit_Company.CompanyName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Category_UID }) |
            @Html.ActionLink("Details", "Details", new { id=item.Category_UID }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.Category_UID }, new { onclick = "return confirm('Are you sure you want to delete?');" })
        </td>
    </tr>
}

</table>
